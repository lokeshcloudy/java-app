def gv

pipeline {
    agent any
    stages {
        stage("init") {
            steps {
                script {
                    gv = load "script.groovy"
                }
            }
        }
        stage("init") {
            steps {
                script {
                    echo "building jar"
                }
            }
        }
        stage("init") {
            steps {
                script {
                    echo "loging into docker"
                }
            }
        }
        stage("init") {
            steps {
                script {
                    echo "building image"
                }
            }
        }
        stage("init") {
            steps {
                script {
                    echo "deploying image"
                }
            }
        }
    }

}
